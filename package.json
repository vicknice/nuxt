    "vue-multiselect": "^2.1.6",
    "vuelidate": "^0.7.6"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "eslint"
    ],
    "*.+(js|json|css|html|md|ts|tsx)": [
      "prettier --write"
    ],
    "**/*.+(js|json|css|html|md)": [
      "jest --findRelatedTests"
    ]
  }
}
