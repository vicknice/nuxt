  "name": "@design-coder/example-nuxt",
  "description": "My sandbox for exploring code and markup",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev-ts": "nuxt-ts",
    "build-ts": "nuxt-ts build",
    "generate-ts": "nuxt-ts generate",
    "start-ts": "nuxt-ts start",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "nuxt",
    "dev2": "nodemon -w server.js -w nuxt.config.js server.js",
    "build": "nuxt build",
    "build:css": "postcss -o assets/css/application.css assets/css/tailwind.css",
    "start": "npm-run-all --parallel start:*",
    "start:client": "nuxt start",
    "generate": "nuxt generate",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "jest": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:8686 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8686 cy:open",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|css|html|md|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "lambda-dev": "cross-env NODE_ENV=development yarn lambda-build && cross-env NODE_ENV=development yarn lambda-serve",
    "lambda-build": "netlify-lambda build ./functions",
    "lambda-serve": "netlify-lambda serve ./functions",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:jest": "jest --config jest.lint.js",
    "validate": "npm run test:coverage && npm run test:e2e:run",
    "netlify": "npm-run-all --parallel test:coverage build && cp -r coverage/lcov-report dist/lcov-report",
    "setup": "npm install && npm run validate",
    "prebuild:storybook": "npm run test:generate-output",
    "storybook": "nuxt-storybook",
    "build:storybook": "nuxt storybook build",
    "predeploy": "npm run build:storybook",
    "deploy": "gh-pages -d storybook-static/",
    "view:tailwind-config": "tailwind-config-viewer -o",
    "build:tailwind-config": "tailwind-config-viewer export ./.tailwind-config",
    "color-tokens": "a11y-color-tokens --outputDirPath='assets/css' --tokenOutputFormat='css' --outputFilename='theme-colors.css' --colorTokensPath='config/__DEFAULT__/color-tokens.js'"
  },
