    "tailwindcss-elevation": "^1.0.1",
    "tailwindcss-scrims": "^1.0.0",
    "tailwindcss-spinner": "^1.2.0",
    "tailwindcss-theming": "^2.4.3",
    "test-data-bot": "^0.8.0",
    "vee-validate": "^2.2.15",
    "vue-apollo": "^3.0.5",
    "vue-content-placeholders": "^0.2.1",
    "vue-feather-icons": "^5.1.0",
