    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxt/typescript-runtime": "latest",
    "canvas-confetti": "^1.3.2",
    "clipboard": "^2.0.6",
    "core-js": "3.8.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.16.1",
    "json-server": "^0.16.3",
    "lodash.groupby": "^4.6.0",
    "lodash": "^4.17.20",
    "marked": "^1.2.7",
    "netlify-lambda": "^2.0.2",
