    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "autoprefixer": "^9.8.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cypress": "^6.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
